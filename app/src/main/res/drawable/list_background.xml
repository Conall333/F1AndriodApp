<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_pressed="false">
        <shape android:shape="rectangle">


            <padding
                android:bottom="20dp"
                android:left="20dp"
                android:right="20dp"
                android:top="20dp">
            </padding>
            <stroke
                android:color="#fff"
                android:width="2dp"
                ></stroke>

            <gradient
                android:startColor="#b3f9ee"
                android:endColor="#ff8789"
                android:angle="45">

            </gradient>




        </shape>
    </item>

</selector>